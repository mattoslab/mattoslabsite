<div class="col-md-3">
  <div class="thumbnail">
    {% if member.photo_src %}
    <img src="static/images/students/{{ member.photo_src }}" alt="{{ member.name }}">
    {% else %}
    <img src="static/images/students/blank.png" alt="{{ member.name }}">
    {% endif %}
    <div class="caption">
      <h3>{{ member.name }}</h3>
      {% if member.description %}{{ member.description|markdown }}{% endif %}
    </div>
  </div>
</div>
